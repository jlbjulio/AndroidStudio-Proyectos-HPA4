            android:fromXDelta="-120dp"
            android:fromYDelta="-150dp"
            android:toXDelta="0"
            android:toYDelta="0" />
        <!-- Paso 20: Diagonal Arriba Derecha -->
        <translate
            android:startOffset="8100"
            android:duration="400"
            android:fromXDelta="0"
            android:fromYDelta="0"
            android:toXDelta="120dp"
            android:toYDelta="-150dp" />
        <!-- Paso 21: Regreso al origen desde Arriba Derecha -->
        <translate
            android:startOffset="8500"
            android:duration="400"
            android:fromXDelta="120dp"
            android:fromYDelta="-150dp"
            android:toXDelta="0"
            android:toYDelta="0" />

        <!-- Paso 22: Diagonal Abajo Izquierda -->
        <translate
            android:startOffset="8900"
            android:duration="400"
            android:fromXDelta="0"
            android:fromYDelta="0"
            android:toXDelta="-120dp"
            android:toYDelta="150dp" />
        <!-- Paso 23: Regreso al origen desde Abajo Izquierda -->
        <translate
            android:startOffset="9300"
            android:duration="400"
            android:fromXDelta="-120dp"
            android:fromYDelta="150dp"
            android:toXDelta="0"
            android:toYDelta="0" />
        <!-- Paso 24: Diagonal Abajo Derecha -->
        <translate
            android:startOffset="9700"
            android:duration="400"
            android:fromXDelta="0"
            android:fromYDelta="0"
            android:toXDelta="120dp"
            android:toYDelta="150dp" />
        <!-- Paso 25: Regreso al origen desde Abajo Derecha -->
        <translate
            android:startOffset="10100"
            android:duration="400"
            android:fromXDelta="120dp"
            android:fromYDelta="150dp"
            android:toXDelta="0"
            android:toYDelta="0" />
        <!-- Paso 26: Diagonal Arriba Derecha -->
        <translate
            android:startOffset="10500"
            android:duration="400"
            android:fromXDelta="0"
            android:fromYDelta="0"
            android:toXDelta="120dp"
            android:toYDelta="-150dp" />
        <!-- Paso 27: Regreso al origen desde Arriba Derecha -->
        <translate
            android:startOffset="10900"
            android:duration="400"
            android:fromXDelta="120dp"
            android:fromYDelta="-150dp"
            android:toXDelta="0"
            android:toYDelta="0" />
        <!-- Paso 28: Diagonal Arriba Izquierda -->
        <translate
            android:startOffset="11300"
            android:duration="400"
            android:fromXDelta="0"
            android:fromYDelta="0"
            android:toXDelta="-120dp"
            android:toYDelta="-150dp" />
        <!-- Paso 29: Regreso al origen desde Arriba Izquierda -->
        <translate
            android:startOffset="11700"
            android:duration="400"
            android:fromXDelta="-120dp"
            android:fromYDelta="-150dp"
            android:toXDelta="0"
            android:toYDelta="0" />



    </set>
</set>